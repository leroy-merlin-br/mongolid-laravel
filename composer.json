{
    "name": "leroy-merlin-br/mongolid-laravel",
    "description": "Easy, powerful and ultrafast MongoDB ODM for Laravel.",
    "keywords": ["odm","mongodb","nosql","laravel","illuminate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        },
        {
            "name": "Guilherme Guitte",
            "email": "guilherme.guitte@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "leroy-merlin-br/mongolid": "2.0.0-beta12",
        "illuminate/support": "^5.2",
        "illuminate/auth": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9",
        "orchestra/testbench": "^3.2",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MongolidLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.0-dev"
        }
    }
}
