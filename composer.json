{
    "name": "leroy-merlin-br/mongolid-laravel",
    "description": "Easy, powerful and ultrafast MongoDB ODM for Laravel.",
    "keywords": ["odm", "mongodb", "nosql", "laravel", "illuminate", "nosql", "active record", "data mapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        },
        {
            "name": "Guilherme Guitte",
            "email": "guilherme.guitte@gmail.com"
        },
        {
            "name": "BoitatÃ¡",
            "email": "boitata@leroymerlin.com.br"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-mongodb": "*",
        "mongodb/mongodb": "^1.4",
        "leroy-merlin-br/mongolid": "^3.0",
        "illuminate/support": "^5.4 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/auth": "^5.4 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/queue": "^5.4 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.0",
        "leroy-merlin-br/coding-standard": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
            "Mongolid\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/Unit/Migrations/stubs/DropAdmin.php",
            "tests/Unit/Migrations/stubs/MigrationCreatorFakeMigration.php"
        ],
        "psr-4": {
            "Mongolid\\Laravel\\": "tests/Unit",
            "Mongolid\\Laravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.1.x-dev",
            "dev-v3": "v3.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Mongolid\\Laravel\\Providers\\MongolidServiceProvider",
                "Mongolid\\Laravel\\Providers\\MigrationServiceProvider"
            ],
            "aliases": {
                "MongoLid": "Mongolid\\Laravel\\Model"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "leroy-merlin-br/mongolid-passport": "Mongolid Passport provides OAuth2 server support to Laravel and Mongolid."
    }
}
